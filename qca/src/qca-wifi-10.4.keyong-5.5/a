
find . -name *.o >b

while read line
do
echo $line
rm -fr $line
done <b



find . -name *.ko >b

while read line
do
echo $line
rm -fr $line
done <b


find . -name *.mod.c>b

while read line
do
echo $line
rm -fr $line
done <b


find . -name *.o.cmd > c


while read line
do
echo $line
rm -f $line
done <c



