
config system
	option hostname 'tz'
	option conloglevel '8'
	option cronloglevel '8'
	option zonename 'Asia/Shanghai'
	option timezone 'CST-8'

config timeserver 'ntp'
	list server 's2m.time.edu.cn'
	list server 'ntp.sjtu.edu.cn'
	list server '2.openwrt.pool.ntp.org'
	list server '3.openwrt.pool.ntp.org'
	option enabled '1'
	option enable_server '0'

config led 'led_wan'
	option name 'WAN'
	option sysfs 'ap147:green:wan'
	option trigger 'netdev'
	option dev 'eth1'
	option mode 'link tx rx'
	option default '0'

config led 'led_lan1'
	option name 'LAN1'
	option sysfs 'ap147:green:lan1'
	option port_mask '0x10'
	option default '1'
	option trigger 'none'

config led 'led_lan2'
	option name 'LAN2'
	option sysfs 'ap147:green:lan2'
	option port_mask '0x08'
	option default '1'
	option trigger 'none'

config led 'led_lan3'
	option name 'LAN3'
	option sysfs 'ap147:green:lan3'
	option port_mask '0x04'
	option default '1'
	option trigger 'none'

config led 'led_lan4'
	option name 'LAN4'
	option sysfs 'ap147:green:lan4'
	option port_mask '0x02'
	option default '1'
	option trigger 'none'

config led 'led_wlan2g'
	option name 'WLAN 2.4 GHz'
	option sysfs 'ap147:green:wlan-2g'
	option default '0'
	option trigger 'netdev'
	option dev 'radio0'
	option mode 'link tx rx'
	option dev 'ath0'
