config wifi-device  radio0
	option type	qcawifi
	option channel	auto
	option macaddr	78:d3:8d:fe:b9:8e
	option hwmode	11gn
	option country  CN
	option frag     2346
	option rts      2347
	option txpower  23
	option htmode   HT20
	list ht_capab SHORT-GI-20
	list ht_capab SHORT-GI-40
	list ht_capab RX-STBC1
	list ht_capab DSSS_CCK-40
#	# REMOVE THIS LINE TO ENABLE WIFI:
#	option disabled 1
        option rssi_thres '-85'

config wifi-iface
	option device	radio0
	option network	lan
	option mode	ap
	option ssid     wireless_2G
	option encryption none
	option key      ""
	option key1     ""
	option key2     ""
	option key3     ""
	option key4     ""
	option hidden   0
	option wmm      1
	option wds      0
	option disabled 0
	option isolate  0
	option maxassoc 64
	option wps_pushbutton 1
	option shortgi  1
	


config wifi-iface
	option device	radio0
	option network	lan
	option mode	ap
	option ssid     wireless_2G-01
	option encryption none
	option key      ""
	option key1     ""
	option key2     ""
	option key3     ""
	option key4     ""
	option hidden   0
	option wmm      1
	option wds      0
	option disabled 1
	option isolate  0
	option maxassoc 64
	option wps_pushbutton 1
	option shortgi  1



config wifi-device  radio1
	option type	qcawifi
	option channel	149
	option macaddr	00:03:7f:4f:00:16
	option hwmode	11ac
	option country  CN
	option frag     2346
	option rts      2347
	option txpower  23
	option htmode   HT80
	list ht_capab SHORT-GI-20
	list ht_capab SHORT-GI-40
	list ht_capab RX-STBC1
	list ht_capab DSSS_CCK-40
#	# REMOVE THIS LINE TO ENABLE WIFI:
#	option disabled 1
	 option rssi_thres '-85'

config wifi-iface
	option device	radio1
	option network	lan
	option mode	ap
	option ssid     wireless_5.8G
	option encryption none
	option key      ""
	option key1     ""
	option key2     ""
	option key3     ""
	option key4     ""
	option hidden   0
	option wmm      1
	option wds      0
	option disabled 0
	option isolate  0
	option maxassoc 64
	option wps_pushbutton 1
	option shortgi  1

config wifi-iface
	option device	radio1
	option network	lan
	option mode	ap
	option ssid     wireless_5.8G_01
	option encryption none
	option key      ""
	option key1     ""
	option key2     ""
	option key3     ""
	option key4     ""
	option hidden   0
	option wmm      1
	option wds      0
	option disabled 1
	option isolate  0
